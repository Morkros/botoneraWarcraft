            "unidades": {
                "soldadoRaso": {
                    "nombre": "soldado Raso",
                    "icono": "../img/unitIcon/BTNFootman.png",
                    "frases": [
                        {
                            "texto": "Si, mi señor",
                            "audio": "../sounds/humans/soldadoRaso/footmanYes1.wav"
                        },
                        {
                            "texto": "Rupert, te necesito",
                            "audio": "../sounds/humans/soldadoRaso/footmanPissed3.wav"
                        },
                        {
                            "texto": "Dejádmelos a mí",
                            "audio": "footmanYesAttack1.wav"
                        },
                        {
                            "texto": "Al ataque!",
                            "audio": "footmanYesAttack2.wav"
                        },
                        {
                            "texto": "A las armas!",
                            "audio": "footmanYesAttack3.wav"
                        },
                        {
                            "texto": "No me chilles que no te veo",
                            "audio": "footmanPissed1.wav"
                        },
                        {
                            "texto": "Pilla la espada y con la horda acaba",
                            "audio": "footmanPissed2.wav"
                        },
                        {
                            "texto": "Nadie es perfecto, por suerte",
                            "audio": "footmanPissed4.wav"
                        },
                        {
                            "texto": "Listo para la acción",
                            "audio": "footmanReady1.wav"
                        },
                        {
                            "texto": "Por Lordaeron",
                            "audio": "footmanWarcry1.wav"
                        },
                        {
                            "texto": "¿Órdenes?",
                            "audio": "footmanWhat1.wav"
                        },
                        {
                            "texto": "¿Qué necesitas?",
                            "audio": "footmanWhat2.wav"
                        },
                        {
                            "texto": "Di la palabra",
                            "audio": "footmanWhat4.wav"
                        }
                    ]
                },
                "arthasHumano": {
                    "nombre": "Arthas Menethil",
                    "icono": "../img/unitIcon/BTNArthas.png",
                    "frases": [
                        {
                            "texto": "La luz es mi fuerza",
                            "audio": "arthasWhat1.wav"
                        },
                        {
                            "texto": "No es necesario hacer una reverencia",
                            "audio": "arthasWhat4.wav"
                        },
                        {
                            "texto": "Un plan sólido",
                            "audio": "arthasYes3.wav"
                        },
                        {
                            "texto": "Estúpido bellaco",
                            "audio": "arthasYesAttack1.wav"
                        },
                        {
                            "texto": "Siente mi cólera",
                            "audio": "arthasYesAttack5.wav"
                        },
                        {
                            "texto": "Ha llegado la justicia",
                            "audio": "arthasYesAttack6.wav"
                        },
                        {
                            "texto": "Hay que purgar toda la ciudad",
                            "audio": "H06Arthas06.wav"
                        },
                        {
                            "texto": "¿Tengo que considerarlo como un acto de traición?",
                            "audio": "H06Arthas10.wav"
                        },
                        {
                            "texto": "No tienes remedio",
                            "audio": "arthasYesAttack3.wav"
                        },
                        {
                            "texto": "Por mi padre, el rey",
                            "audio": "arthasYesAttack2.wav"
                        },
                        {
                            "texto": "Por el honor",
                            "audio": "arthasYes4.wav"
                        },
                        {
                            "texto": "Por supuesto",
                            "audio": "arthasYes2.wav"
                        },
                        {
                            "texto": "Lucho por la luz",
                            "audio": "arthasWhat3.wav"
                        },
                        {
                            "texto": "¿Hay algún peligro?",
                            "audio": "arthasWhat2.wav"
                        },
                        {
                            "texto": "Por Lordaeron",
                            "audio": "arthasWarcry1.wav"
                        },
                        {
                            "texto": "Se lo que estoy haciendo",
                            "audio": "arthasPissed6.wav"
                        },
                        {
                            "texto": "Que la luz ilumine mi camino",
                            "audio": "arthasPissed3.wav"
                        },
                        {
                            "texto": "Debería estar yo al mando",
                            "audio": "arthasPissed5.wav"
                        },
                        {
                            "texto": "Que la luz me de fuerza",
                            "audio": "arthasPissed4.wav"
                        },
                        {
                            "texto": "Maldito seas Mal'Ganis, te perseguire hasta los confines de la tierra",
                            "audio": "h06arthas46.wav"
                        }
                    ]
                },
                "kaelthas": {
                    "nombre": "Kael'thas Sunstrider",
                    "icono": "../img/unitIcon/BTNHeroBloodElfPrince.png",
                    "frases": [
                        {
                            "texto": "Estoy nerviosillo",
                            "audio": "kaelWhat1.wav"
                        },
                        {
                            "texto": "Mi gente debe ser vengada",
                            "audio": "kaelWhat3.wav"
                        },
                        {
                            "texto": "Percibo algo escurridizo",
                            "audio": "kaelWhat5.wav"
                        },
                        {
                            "texto": "Esto es demasiado fácil",
                            "audio": "kaelYesAttack3.wav"
                        },
                        {
                            "texto": "Venganza",
                            "audio": "kaelYesAttack2.wav"
                        },
                        {
                            "texto": "Que el fuego os purgue",
                            "audio": "kaelYesAttack1.wav"
                        },
                        {
                            "texto": "¿Qué pasa?",
                            "audio": "kaelWhat2.wav"
                        },
                        {
                            "texto": "De mi tierra no quedan más que cenizas y dolor",
                            "audio": "kaelPissed1.wav"
                        },
                        {
                            "texto": "Mi sed de magia me esta nublando el juicio",
                            "audio": "kaelPissed2.wav"
                        },
                        {
                            "texto": "Solo siento odio y furia",
                            "audio": "kaelPissed3.wav"
                        },
                        {
                            "texto": "La Alianza ha fallado a mi gente",
                            "audio": "kaelPissed4.wav"
                        },
                        {
                            "texto": "Estoy esperando",
                            "audio": "kaelReady1.wav"
                        },
                        {
                            "texto": "Endo-enah Quel'thalas",
                            "audio": "kaelWarcray1.wav"
                        },
                        {
                            "texto": "Deberíamos movernos en breve",
                            "audio": "kaelWhat4.wav"
                        },
                        {
                            "texto": "Haré todo lo posible",
                            "audio": "kaelYes3.wav"
                        }
                    ]
                },
